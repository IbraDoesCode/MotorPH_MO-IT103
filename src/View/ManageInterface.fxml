<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="450.0" prefWidth="650.0" style="-fx-background-color: #f6f7eb;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ManageInterfaceController">
   <children>
      <TableView fx:id="employeeTable" fixedCellSize="0.0" layoutX="25.0" layoutY="75.0" prefHeight="300.0" prefWidth="600.0">
         <columns>
            <TableColumn fx:id="employeeID_Column" editable="false" prefWidth="85.0" resizable="false" text="Employee #" />
            <TableColumn fx:id="lastName_Column" editable="false" prefWidth="85.0" text="Last Name" />
            <TableColumn fx:id="firstName_Column" editable="false" prefWidth="85.0" resizable="false" text="First Name" />
            <TableColumn fx:id="sssNo_Column" editable="false" prefWidth="85.0" resizable="false" sortable="false" text="SSS No" />
            <TableColumn fx:id="philhealthNo_Column" editable="false" prefWidth="85.0" resizable="false" sortable="false" text="Philhealth No" />
            <TableColumn fx:id="pagibigNo_Column" editable="false" prefWidth="85.0" resizable="false" sortable="false" text="Pagibig No" />
            <TableColumn fx:id="tinNo_Column" editable="false" prefWidth="85.0" resizable="false" sortable="false" text="Tin No" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
